{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["<", ">"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["<", ">"],
        ["\"", "\""]
    ],
    "folding": {
        "markers": {
            "start": "^(GET|POST|PUT|DELETE|PATCH|HEAD|OPTIONS|CONNECT|TRACE|curl|@tests|@script)\\s+",
            "end": "^#{3,}$"
        }
    }
}